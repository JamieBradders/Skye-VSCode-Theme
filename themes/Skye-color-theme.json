{
  "name": "Skye",
  "type": "dark",
  "colors": {
    // Global Styles
    "focusBorder": "#2f405d75",

    // Activity Bar Styles
    "activityBar.background": "#1e293b",
    "activityBarBadge.background": "#f472b6",
    "activityBar.foreground": "#93c5fd",
    "activityBar.activeBorder": "#f472b6",

    // Button Styles
    "button.background": "#27354c",

    // Editor Styles
    "editor.background": "#1e293b",
    "editor.foreground": "#f8fafc",
    "editorGroupHeader.tabsBackground": "#1e293b",
    "editorLineNumber.foreground": "#475569",
    "editorOverviewRuler.border": "#151d2a",

    // Input Styles
    "input.background": "#27354c",
    "input.border": "#384c6e",

    // Keybinding Styles
    "keybindingLabel.background": "#151d2a",

    // Menu Styles
    "menu.background": "#1e293b",

    // Panel styles
    "panel.border": "#151d2a",
    "panelInput.border": "#f472b6",

    // Sidebar styles
    "sideBar.background": "#1e293b",
    "sideBar.border": "#151d2a",
    "sideBarSectionHeader.background": "#1e293b",

    // Scrollbar styles
    "scrollbar.shadow": "#1e293b",

    // List styles
    "list.activeSelectionBackground": "#2f405d",
    "list.focusOutline": "#f472b6",
    "list.hoverBackground": "#384c6e",

    // Quick Input / Command Palette
    "quickInput.background": "#151d2a",
    "quickInput.foreground": "#64748B",

    // Scrollbar Styles
    "scrollbarSlider.background": "#2f405d75",
    "scrollbarSlider.hoverBackground": "#2f405d",
    "scrollbarSlider.activeBackground": "#2f405d",

    // Status Bar Styles
    "statusBar.background": "#151d2a",
    "statusBar.noFolderBackground": "#151d2a",
    "statusBar.debuggingBackground": "#151d2a",

    // Tab Styles
    "tab.activeBackground": "#151d2a",
    "tab.activeBorder": "#f472b6",
    "tab.inactiveBackground": "#1e293b",
    "tab.hoverBackground": "#27354c",

    // Title Bar Styles
    "titleBar.activeBackground": "#1e293b",

    // Welcome Page Styles
    "welcomePage.tileBackground": "#1e293b",
    "welcomePage.tileHoverBackground": "#1e293b"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#CBD5E1"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "JS Curly Braces",
      "scope": ["punctuation.definition.block.js"],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Punctuation Tag",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#64748b"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "SCSS Tag",
      "scope": ["source.css.scss entity.name.tag"],
      "settings": {
        "foreground": "#CBD5E1"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Arrays",
      "scope": ["source.js meta.brace.square.js"],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#facc15"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#facc15"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F472B6"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#cbd5e1"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F472B6"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["constant.numeric.css"],
      "settings": {
        "foreground": "#7DD3FC"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#facc15"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#94a3b8"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F472B6"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#facc15"
      }
    },
    {
      "name": "JS Variable Delcaration",
      "scope": [
